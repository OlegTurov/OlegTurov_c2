<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>hello</servlet-name>
        <servlet-class>com.example.realproject.first.HelloServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hello</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>result</servlet-name>
        <servlet-class>com.example.realproject.first.ResultServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>result</servlet-name>
        <url-pattern>/result</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>calcres</servlet-name>
        <servlet-class>com.example.realproject.second.CalcresServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>calcres</servlet-name>
        <url-pattern>/calcres</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>calculator</servlet-name>
        <servlet-class>com.example.realproject.second.CalculatorServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>calculator</servlet-name>
        <url-pattern>/calculator</url-pattern>
    </servlet-mapping>
</web-app>